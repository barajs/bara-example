{
  "name": "bara_demo",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://example.barajs.dev",
  "scripts": {
    "dev": "run-p dev:web dev:desktop",
    "dev:web": "run-s compile do:web",
    "dev:desktop": "cross-env BROWSER=none concurrently \"yarn dev:web\" \"yarn compile:desktop -w\" \"wait-on http://localhost:3000 && yarn start:desktop\"",
    "start:mobile": "node node_modules/react-native/local-cli/cli.js start",
    "start:web": "react-scripts start",
    "start:desktop": "nodemon --watch dist --exec \"electron ./dist\"",
    "do:desktop": "concurrently \"yarn dev:web\" \"yarn compile:desktop -w\" \"wait-on http://localhost:3000 && yarn start:desktop\"",
    "do:web": "concurrently --kill-others \"yarn compile -w\" \"yarn start:web\"",
    "compile": "tsc -b",
    "compile:web": "react-scripts build",
    "compile:desktop": "tsc -b ./tsconfig.desktop.json",
    "test": "jest"
  },
  "dependencies": {
    "bara": "2.2.0",
    "bara-react": "1.3.2",
    "electron-store": "3.2.0",
    "electron-updater": "4.0.6",
    "jsonfile": "5.0.0",
    "mkdirp": "0.5.1",
    "react": "16.8.3",
    "react-dom": "16.8.6",
    "react-native": "0.59.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/electron-store": "1.3.1",
    "@types/jest": "^24.0.11",
    "@types/jsonfile": "5.0.0",
    "@types/mkdirp": "0.5.2",
    "@types/react": "^16.8.13",
    "@types/react-native": "^0.57.43",
    "@types/react-test-renderer": "^16.8.1",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "electron": "4.1.0",
    "electron-builder": "20.39.0",
    "electron-devtools-installer": "2.2.4",
    "metro-react-native-babel-preset": "^0.53.1",
    "nodemon": "1.18.11",
    "npm-run-all": "4.1.5",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "16.8.3",
    "typescript": "^3.4.3",
    "wait-on": "3.2.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "files": [
      "dist"
    ]
  }
}
